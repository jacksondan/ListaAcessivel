<%@ page language="java" contentType="text/html; charset=ISO-8859-1"
	pageEncoding="ISO-8859-1"%>
			
<!DOCTYPE HTML>
<html lang="pt-br">
<head>
<meta charset="ISO-8859-1">
<title>ListaAcessível - Cadastre-se</title>
<link rel="stylesheet" type="text/css" href="css/style.css">
<link rel="shortcut icon" href="images/logtop.png" />
</head>
<body>
	<div id="container">
		<div id="header-wrapper">
			<div id="header" class="container">
				<h1 id="logo">
				<h1 id="logo"><a href="index.jsp"><img src="images/ic_logo_web.png" alt= "Logo Lista acessível"  style="width:6.6em;heigth:16em;;"></a></h1>

				</h1>

			</div>
		</div>
		
		<div id="content">
			<header class="major">
				<h2>Fale Conosco!</h2>
			</header>
		
	
		<form action="" method="post">
			<fieldset>
					<legend>Preencha os dados abaixo e envie-nos a sua mensagem</legend>
					
					<label for="nome">Nome</label>
					<br>
					<input name="nome" id="nome" placeholder="Digite seu nome" type="text"size="30" />
					</p>
						<label for="email">Email</label>
					<br>
					<input name="email" id="email" placeholder="Digite seu Email" type="text"size="30" />
					</p>
					<p>
					<label for="mensagem">Mensagem</label>
					<textarea name="mensagem" id="mensagem"placeholder="Digite aqui sua Mensagem.." cols="50" rows="4"></textarea>
					</p>
					</fieldset>
			
					<ul class="actions">
						<li><input type="submit" value="Enviar" /></li>
						<li><input type="reset" value="Limpar" /></li>
					</ul>
			
		</form>
		
		</div>
		<div id="footer">
			<div id="copyright" class="container">
				<ul class="icons">
					<li>&copy; TechBin. Todos os direitos reservados.</li>
					<li>Design: <a href="http://facebook.com/Edwardhll">Eduardo
							Andrade</a></li>
				</ul>
			</div>
		</div>
	</div>
</body>
</html>
	

